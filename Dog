package zoo;

public class Dog extends Animal {

    public void bark() {
        System.out.println("woof woof");
    }

    public Dog(int ag2, int weight, String dogName) {
        super(ag2,weight,dogName);
    }


    public void printAll() {

        System.out.println("i am dog");
    }

    public void saySomething() {
        System.out.println("Woof Woof!");
    }

}
